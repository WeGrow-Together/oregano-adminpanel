<form action="#" method="POST" id="addbusiness" enctype="multipart/form-data">
    <div class="card-body ">
        <div class="form-group">
            <div class="filter ">
                <a href="/business">
                    <i class="fas fa-angle-double-left "></i>All Business</a>
            </div>
            <br>
            <div class="form-title text-center ">
                <h2 class="text-dark ">New business</h2>
                <span class="text-dark ">Use the below form to create new business</span>
            </div>
            <label for="Email ">Email address</label>
            <input type="email" name="email" value="" class="form-control " id="exampleInputEmail1 " placeholder=" example@gmail.com ">
        </div>
        <div class="form-group">
            <label for="cname ">Company Name</label>
            <input type="text " name="cname" value="" class="form-control " id="cname" placeholder="Company Name ">
        </div>
        <div class="form-group">
            <label for="oname ">Owner Name</label>
            <input type="text " name="oname" value="" class="form-control " id="oname" placeholder="Owner Name ">
        </div>

        <div class="form-group">
            <label for="mobile ">Mobile On</label>
            <input type="number" name="mobile" value="" class="form-control " id="mobile" placeholder="Mobile Number">
        </div>
        <div class="form-group">
            <label for="trade ">Trade licence</label>
            <input type="file" name="files" value="" class="form-control ">
        </div>
        <div class="form-group">
            <label for="company ">Type of company</label>
            <select name="company" id="company">
                <option value="it">IT</option>
                <option value="marketing">Marketing</option>
                <option value="business">Business</option>
                
              </select>
        </div>
        <div class="form-group">
            <label for="card ">PAN Card Number</label>
            <input type="number" name="card" value="" class="form-control " id="card" placeholder="PAN Number">
        </div>
        <div class="form-group">
            <label for="panimage ">PAN Card Image</label>
            <input type="file" name="panimage" value="" class="form-control ">
        </div>
        <div class="form-group">
            <label for="agreeimage ">Agreement copy Image</label>
            <input type="file" name="agreeimage" value="" class="form-control ">
        </div>
        <div class="form-group">
            <label for="location ">Business Location</label>
            <input type="text " name="location" value="" class="form-control " id="location" placeholder="Business location">
        </div>
        <div class="form-group">
            <label for="address ">Business Address</label>
            <input type="text " name="address" value="" class="form-control " id="address" placeholder="Business Address">
        </div>
        <div class="form-group">
            <label for="employee ">Number of Employees</label>
            <select name="employee" id="employee">
                <option value="ten">Less than 10</option>
                <option value="thirty">Less than 30</option>
                <option value="thirtyplus">More than 30</option>
              </select>
        </div>
        <div class="form-group">
            <label for="gst ">GST number</label>
            <input type="number" name="gst" value="" class="form-control " id="gst" placeholder="GST Number">
        </div>
        <div class="form-group">
            <label for="account ">Account number</label>
            <input type="number" name="account" value="" class="form-control " id="account" placeholder="Account Number">
        </div>
        <div class="form-group">
            <label for="code ">ISFC code</label>
            <input type="text" name="code" value="" class="form-control " id="code" placeholder="ISFC code">
        </div>
        <div class="form-group">
            <label for="service ">Types of services</label>
            <select name="service" id="service">
                <option value="italian">Italian Platter</option>
                <option value="bengali">Bengali Platter</option>
                <option value="diet">Gym Diet</option>
                <option value="mughlai">Mughlai</option>
                <option value="chinese">Chinese</option>
              </select>
        </div>
        <div class="form-group">
            <label for="date ">Established from</label>
            <input type="date" name="date" value="" class="form-control " id="date">
        </div>

        <div class="form-group">
            <label for="status">Status</label>
            <div class="radio inline">
                <input type="radio" name="status" value="Active" id="radio-4">
                <label for="radio-4">Active</label>
            </div>

            <div class="radio inline">
                <input type="radio" name="status" value="Inactive" id="radio-5">
                <label for="radio-4">Inactive</label>
            </div>
        </div>
    </div>
    <!-- /.card-body -->

    <div class="card-footer ">
        <button type="submit " id="submit" class=" btn btn-primary ">Submit</button>
    </div>

</form>